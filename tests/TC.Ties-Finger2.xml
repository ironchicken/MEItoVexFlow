                        </staffGrp>
                    </scoreDef>
                    <section>
                        <measure n="1" xml:id="d1e12">
              <!-- By unraveling the layer structure it can be determined which
                ties cut across layers and which do not. Those that do not can
                be represented with @tie, while cross-layer ties must be
                captured using tie elements. If desired, ties that do not cross
                layers may be represented using tie elements as well.             
              -->
                            <staff n="1">
                                <layer n="1">
                                    <rest xml:id="t2er01" dur="2"/>
                                    <beam>
                                        <note xml:id="a" dur="8" pname="e" oct="3" stem.dir="up"/>
                                        <note xml:id="b" dur="8" pname="f" accid="s" oct="3" stem.dir="up"/>
                                        <note xml:id="t2e01" dur="8" pname="a" accid="n" oct="3" stem.dir="up"/>
                                        <note xml:id="t2e02" dur="8" pname="e" accid="f" oct="4" stem.dir="up" tie="i"/>
                                    </beam>
                                </layer>
                                <layer n="2">
                                    <rest xml:id="t2er02" dur="2"/>
                                    <rest xml:id="t2er03" dur="8"/>
                                    <note xml:id="t2e03" dur="8" pname="f" accid="s" oct="3" stem.dir="down"/>
                                    <note xml:id="t2e04" dur="4" pname="a" oct="3"/>
                                </layer>
                                <layer n="3">
                                    <rest xml:id="t2er04"  dur="2"/>
                                    <rest xml:id="t2er05"  dur="4"/>
                                    <note xml:id="c" dur="4" pname="f" oct="3" stem.dir="down"/>
                                </layer>
                                <layer n="4">
                                    <rest xml:id="t2er06"  dur="2" dots="1"/>
                                    <note xml:id="t2e05" dur="2" pname="e" oct="3" ho="2"/>
                                </layer>
                            </staff>
                            <staff n="2">
                                <layer n="1">
                                    <beam>
                                        <note xml:id="t2e06" dur="8" pname="c" oct="2"/>
                                        <note xml:id="t2e07" dur="8" pname="f" accid="s" oct="2"/>
                                        <note dur="8" pname="a" oct="2" tie="i" xml:id="d"/>
                                        <note xml:id="t2e09" dur="8" pname="c" oct="3" tie="i"/>
                                    </beam>
                                    <note xml:id="t2e10" dur="2" pname="c" oct="3" tie="m"/>
                                </layer>
                                <layer n="2">
                                    <note xml:id="t2e11" dur="2" pname="c" oct="2" tie="m"/>
                                    <chord dur="2">
                                        <note xml:id="t2e12" pname="c" oct="2" tie="i" dur="2"/>
                                        <note xml:id="t2e13" pname="f" oct="2" tie="i" dur="2"/>
                                        <note xml:id="e" pname="a" oct="2" tie="i" dur="2"/>
                                    </chord>
                                </layer>
                                <layer n="3">
                                    <rest xml:id="t2er07"  dur="8"/>
                                    <note xml:id="t2e14" stem.dir="down" dur="8" pname="f" oct="2" tie="i"/>
                                    <note xml:id="t2e15" stem.dir="down" ho="2" dur="4" pname="f" oct="2" tie="i"/>
                                    <note xml:id="t2e16" ho="0" dur="2" pname="f" oct="2"/>
                                </layer>
                                <layer n="4">
                                    <rest xml:id="t2er08"  dur="4"/>
                                    <note xml:id="t2e17" dur="4" pname="a" oct="2"/>
                                </layer>
                            </staff>
              <!-- x-layer tie -->
                            <tie staff="1" startid="b" endid="c" bulge="-1"/>
              <!-- x-layer tie -->
                            <tie staff="1" startid="d" endid="e" bulge="-1"/>
                        </measure>
                        <measure n="2">
                            <staff n="1">
                                <layer n="1">
                                    <chord dur="2">
                                        <note xml:id="t2e18" pname="a" oct="3" dur="2"/>
                                        <note xml:id="t2e19" pname="e" accid="f" oct="4" dur="2"/>
                                    </chord>
                                </layer>
                                <layer n="2">
                                    <note xml:id="t2e20" dur="8" pname="a" oct="3" stem.dir="down"/>
                                    <note xml:id="t2e21" dur="8" pname="f" accid="f" oct="3" stem.dir="down"/>
                                    <note xml:id="t2e22" dur="4" pname="e" accid="f" oct="3" tie="i" stem.dir="down"/>
                                    <note xml:id="t2e23" dur="2" pname="e" accid="f" oct="3" stem.dir="down"/>
                                </layer>
                                <layer n="3">
                                    <rest xml:id="t2er09"  dur="8"/>
                                    <note xml:id="t2e24" dur="8" pname="f" accid="s" oct="3" tie="i" stem.dir="up"/>
                                    <note xml:id="t2e25" dur="4" pname="f" accid="s" oct="3" tie="i" stem.dir="up"/>
                                    <note xml:id="t2e26" dur="2" pname="f" accid="s" oct="3" stem.dir="up"/>
                                </layer>
                            </staff>
                            <staff n="2">
                                <layer n="1">
                                    <note xml:id="t2e27" dur="4" dots="1" pname="c" oct="3"/>
                                    <note xml:id="t2e28" dur="8" pname="c" oct="3" tie="i"/>
                                    <note xml:id="t2e29" dur="4" dots="1" pname="c" oct="3"/>
                                    <note xml:id="t2e30" dur="8" pname="c" oct="3"/>
                                </layer>
                                <layer n="2">
                                    <chord dur="2">
                                        <note xml:id="t2e31" pname="c" oct="2" dur="2"/>
                                        <note xml:id="t2e32" pname="f" oct="2" dur="2"/>
                                        <note xml:id="t2e33" pname="a" oct="2" dur="2"/>
                                    </chord>
                                    <rest xml:id="t2er10"  dur="4"/>
                                    <note xml:id="t2e34" ho="4" dur="4" pname="c" oct="2"/>
                                </layer>
                                <layer n="3">
                                    <rest xml:id="t2er11"  dur="2"/>
                                    <note dur="2" pname="a" oct="2" xml:id="f"/>
                                </layer>
                                <layer n="4">
                                    <rest xml:id="t2er12"  dur="2"/>
                                    <beam>
                                        <note dur="8" pname="a" oct="2" xml:id="g" ho="1.75"/>
                                        <note xml:id="t2e37" dur="8" pname="f" accid="s" oct="2"/>
                                    </beam>
                                </layer>
                                <layer n="5">
                                    <rest xml:id="t2er13"  dur="2"/>
                                    <rest xml:id="t2er14"  dur="8"/>
                                    <note xml:id="t2e01" dur="4" dots="1" pname="f" accid.ges="s" oct="2"/>
                                </layer>
                            </staff>
              <!-- x-layer tie -->
                            <tie staff="2" startid="f" endid="g" bulge="-1"/>
